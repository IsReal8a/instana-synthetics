# Application Definition 
app_name        = "synthetic-pop" # Do NOT enter any spaces
app_environment = "dev"       # Dev, Test, Staging, Prod, etc

# AWS Network
aws_az             = "eu-west-1a"
vpc_cidr           = "10.11.0.0/16"
public_subnet_cidr = "10.11.1.0/24"

# AWS Settings
aws_access_key = "CHANGEME"
aws_secret_key = "CHANGEME"
aws_region     = "eu-west-1"

# Linux Virtual Machine
linux_instance_type               = "t2.micro" # Change this to large or more, micro is default for testing.
linux_associate_public_ip_address = true
linux_root_volume_size            = 20
linux_root_volume_type            = "gp2"
linux_data_volume_size            = 10
linux_data_volume_type            = "gp2"